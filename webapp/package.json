{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json && tsc --noEmit --project ./tsconfig.app.json",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss",
    "stylecheck": "sass -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}'"
  },
  "dependencies": {
    "@ideanick/backend": "workspace:*",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "i": "^0.3.7",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-head": "^3.4.2",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^7.6.3",
    "reset-css": "^5.0.2",
    "sass": "^1.89.2",
    "superjson": "^2.2.2",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "8.47.0",
    "prettier": "^3.0.1",
    "stylelint": "^16.21.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.8.3",
    "vite": "^4.4.5"
  }
}
